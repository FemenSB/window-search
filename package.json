{
  "name": "window-search",
  "displayName": "Window Search",
  "description": "Analyze log files by searching for time windows with begin/end markers",
  "version": "1.0.0",
  "publisher": "FelipeBorsato",
  "repository": {
    "type": "git",
    "url": "https://github.com/FemenSB/window-search/"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "window-search",
          "title": "Window Search",
          "icon": "$(list-tree)"
        }
      ]
    },
    "views": {
      "window-search": [
        {
          "id": "windowSearchView",
          "name": "Window Search",
          "type": "webview"
        }
      ]
    },
    "commands": [
      {
        "command": "window-search.removeMatchingLines",
        "title": "Remove all matching lines"
      },
      {
        "command": "window-search.removeNonMatchingLines",
        "title": "Remove all but matching lines"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "window-search.removeMatchingLines",
          "when": "editorHasSelection",
          "group": "9_cutcopypaste"
        },
        {
          "command": "window-search.removeNonMatchingLines",
          "when": "editorHasSelection",
          "group": "9_cutcopypaste"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
